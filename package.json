    "release": "standard-version"
  },
  "dependencies": {
    "commander": "^6.2.0",
    "jsonparse": "^1.3.1",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "coveralls": "^3.1.0",
    "docpress": "^0.8.2",
    "eslint": "^7.13.0",
    "gh-pages": "^3.1.0",
    "in-publish": "^2.0.1",
    "nyc": "^15.1.0",
    "rollup": "^2.33.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "standard-version": "^9.0.0",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6.13.0"
  },
  "volta": {
    "node": "10.19.0"
  }
