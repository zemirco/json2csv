{
  "name": "json2csv",
  "preferGlobal": "true",
  "version": "2.3.0",
  "description": "Convert json to csv with column titles (while keeping the escape char)",
  "keywords": [
    "json",
    "to",
    "csv",
    "export",
    "convert",
    "parse"
  ],
  "author": {
    "name": "Mirco Zeiss",
    "email": "mirco.zeiss@gmail.com",
    "twitter": "zeMirco"
  },
  "bin": {
    "json2csv": "./bin/json2csv.js"
  },
  "main": "./lib/json2csv.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zeMirco/json2csv"
  },
  "scripts": {
    "test": "node test | tap-spec",
    "format": "node ./node_modules/js-beautify/js/bin/js-beautify.js lib/json2csv.js test/test.js package.json -r --config js-beautify.json",
    "test-coverage": "istanbul cover test/index.js --report lcovonly | tap-spec"
  },
  "dependencies": {
    "commander": "~1.0.5",
    "cli-table": "~0.2.0",
    "async": "~0.2.9"
  },
  "devDependencies": {
    "async": "~0.2.9",
    "istanbul": "^0.3.14",
    "js-beautify": "~1.4.2",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0"
  }
}
